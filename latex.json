    "Insert Figure": {
        "prefix": "figure",
        "body": [
            "\\begin{figure}[H]",
            "    \\centering",
            "    \\includegraphics[scale=0.75]{${1}}",
            "    \\caption{${2}}",
            "    \\label{${3}}",
            "\\end{figure}%"
        ],
        "description": "Insert Figure"
    },

    "Insert Subfigure": {
        "prefix": "subfigure",
        "body": [
        "\\begin{figure}[H]",
        "    \\centering",
        "    \\begin{subfigure}{0.4\\columnwidth}",
        "        \\centering",
        "        \\includegraphics[width=\\columnwidth]{${1}}",
        "        \\caption{${2}}",
        "        \\label{${3}}",
        "    \\end{subfigure}",
        "    \\hspace{5mm}",
        "    \\begin{subfigure}{0.4\\columnwidth}",
        "        \\centering",
        "        \\includegraphics[width=\\columnwidth]{${4}}",
        "        \\caption{${5}}",
        "        \\label{${6}}",
        "    \\end{subfigure}",
        "    \\caption{${7}}",
        "    \\label{${8}}",
        "\\end{figure}%",
        ""
        ],
        "description": ""
    },

    "Insert Table": {
        "prefix": "table",
        "body": [
        "\\begin{table}[H]",
        "    \\centering",
        "    \\caption[]{}",
        "    \\label{}",
        "    \\begin{tabular}{${1}}",
        "        ${2}",
        "    \\end{tabular}",
        "\\end{table}%"
        ],
        "description": ""
    },

    "Insert Reference": {
